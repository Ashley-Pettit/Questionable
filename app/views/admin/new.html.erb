<h1>Add An Event</h1>

<%= form_tag '/event_new', method: 'patch' do %>
  <label>Name:</label>
  <input class="std_input" name='event_title_input'><BR />
  <label>Description:</label>
  <input class="std_input" name='description_input'><BR />
  <label>Speaker:</label>
  <input class="std_input" name='speaker_input'><BR />
  <button class="button">Add Event</button>
<% end %>
